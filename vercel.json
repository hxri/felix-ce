{
  "version": 2,
  "builds": [
    {
      "src": "app/backend/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "runtime": "python3.11",
        "maxDuration": 900
      }
    },
    {
      "src": "app/frontend/package.json",
      "use": "@vercel/next"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/app/backend/wsgi.py"
    }
  ],
  "env": {
    "NEXT_PUBLIC_API_URL": ""
  }
}